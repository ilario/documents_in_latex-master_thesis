mendeley-tags = {bib-tesi,graft from,nmrp,pt},
month = dec,
number = {45},
pages = {16445--53},
pmid = {19860410},
title = {{Hairy poly(3-hexylthiophene) particles prepared via surface-initiated Kumada catalyst-transfer polycondensation.}},
