mendeley-tags = {aggregazione,bib-tesi,chirale,pt},
month = aug,
number = {31},
pages = {11197--202},
pmid = {15280547},
title = {{Twisting macromolecular chains: self-assembly of a chiral supermolecule from nonchiral polythiophene polyanions and random-coil synthetic peptides.}},
